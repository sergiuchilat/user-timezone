{
    "name": "magadanuhak/user-timezone",
    "description": "A simple solution for converting Laravel's default dates attributes to a user's set timezone",
    "type": "timezone, date, dates",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergiu Popa",
            "email": "popa.sergiu920@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4|>=8.0",
        "illuminate/support": ">=7.0",
        "laravel/framework": ">=7.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "LaravelMerax\\UserTimeZone\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMerax\\UserTimeZone\\AppServiceProvider"
            ]
        }
    }
}
